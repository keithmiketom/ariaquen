<!DOCTYPE html>
<html>
<head>
	<!--[if !IE 7]><style type="text/css"> #wrap {display:table;height:100%}</style><![endif]-->
  <title>Ariaquen</title>

  <%= stylesheet_link_tag    "application", :media => "all" %>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
  <script src="//code.jquery.com/jquery-2.0.3.min.js"></script>
  <script src="//code.jquery.com/jquery-1.7.1.min.js"></script>
  <script src="../assets/bjqs-1.3.js"></script>
  <script src="../assets/jquery.js"></script>
  <script src="../assets/libs/index.js"></script>
  <script src="../assets/libs/skel.min.js"></script>
  <script src="../assets/libs/skel-panels.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
  <script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>

  <%= stylesheet_link_tag    "main", :media => "all" %>


  <%= csrf_meta_tags %>
</head>
<body>
	<div id="wrap" class="container">
		
		<%= render 'layouts/header' %> 

	<main id="main">
		<div class="alerts">
				<p class="notice"><%= notice %></p> 
				<p class="alert"><%= alert %></p>
		</div>
		<%= yield %>
	
	</main>
</div><!--end of wrap-->

<%= render 'layouts/footer' %>

<section class="javascript_includes">
  <%= javascript_include_tag "application" %>
</section>


</body>
</html>
